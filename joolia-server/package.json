{
    "name": "joolia-server",
    "version": "2.32.1",
    "description": "Joolia API Server",
    "author": "Joolia GmbH",
    "private": true,
    "scripts": {
        "db:update": "npm run build && bash ./dist/db/update.sh",
        "db:restore": "npm run db:update -- --restore-schema --dev-environment",
        "db:setup": "npm run db:update -- --migrate --no-schema-check --dev-environment",
        "db:sync": "npm run db:update -- --no-schema-check --dev-environment",
        "db:init": "npm run db:update -- --migrate --dev-environment --initialize",
        "migration:dev": "npm run db:update -- --migrate --dev-environment",
        "migration:ci": "npm run db:update -- --migrate --ci-environment",
        "migration:sim": "npm run db:update -- --migrate --prod-environment 127.0.0.1 admin",
        "migration:generate": "npm run build && typeorm migration:generate --dir ./db/migrations --config ./db/configurations/ormconfig-gen-jooliadb.yml",
        "migration:create": "npm run build && typeorm migration:create --dir ./db/migrations --config ./db/configurations/ormconfig-gen-jooliadb.yml",
        "copyall": "copyfiles -e \"./**/*.ts\" -a src/emailTemplates/* src/emailTemplates/**/* ./src/api/docs/**/*.yaml ./test/fixture/**/*.yml ./db/* ./db/**/* ./dist",
        "build": "shx rm -rf dist && tsc -p tsconfig.build.json && npm run copyall",
        "fixtures": "fixtures -c ./db/configurations/ormconfig-fixtures.yml --require ts-node/register ./test/fixture/",
        "clean": "shx rm -rf dist out node_modules",
        "test:dev": "ts-mocha --reporter min --exit --timeout 45000 -r dotenv/config './test/**/*.spec.ts'",
        "test-with-coverage:dev": "nyc --reporter=text mocha --reporter min --exit --timeout 45000 -r dotenv/config -r ts-node/register ./test/**/*.spec.ts",
        "test:ci": "npm run migration:ci && JOOLIA_DB_CONN_LIMIT=1 JOOLIA_DB_HOST=db JOOLIA_DB_LOGGING=false JOOLIA_WS_ENABLED=false JOOLIA_CSURF_ENABLED=false nyc --reporter=text mocha --exit --timeout 45000 ./dist/test/**/*.spec.js",
        "start": "node -r dotenv/config dist/src",
        "start:dev": "ts-node --files -r dotenv/config src/index.ts",
        "start:watch": "nodemon",
        "circular:check": "madge --circular --exclude '(models\\/internal|(.*)Repository)\\.js$' .",
        "lint": "npm run eslint",
        "lint:fix": "npm run eslint:fix && npm run prettier:fix",
        "prettier:cli": "prettier \"**/*.ts\" \"**/*.yaml\" \"**/*.yml\"",
        "prettier:check": "npm run prettier:cli -- -l",
        "prettier:fix": "npm run prettier:cli -- --write",
        "eslint:fix": "npm run eslint -- --fix",
        "eslint": "eslint . --ext .ts"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*.ts": [
            "eslint"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://www.bitbucket.org/ideasuite/joolia-server.git"
    },
    "engines": {
        "npm": ">=6.13.0",
        "node": ">=12.14.0 <=12.18.0"
    },
    "dependencies": {
        "aws-sdk": "^2.455.0",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "csurf": "^1.11.0",
        "email-templates": "^8.0.3",
        "express": "4.16.4",
        "express-query-boolean": "^2.0.0",
        "express-validator": "^5.3.1",
        "express-winston": "^4.1.0",
        "faker": "^5.1.0",
        "filename-reserved-regex": "^2.0.0",
        "filenamify": "^4.1.0",
        "http-status": "^1.4.0",
        "ini": "^1.3.6",
        "jwt-simple": "^0.5.5",
        "lodash": "^4.17.21",
        "moment": "^2.24.0",
        "mysql": "^2.17.1",
        "nodemailer": "^6.4.16",
        "passport": "^0.4.0",
        "passport-cookie": "^1.0.8",
        "passport-jwt": "^4.0.0",
        "pug": "^3.0.2",
        "request": "^2.88.0",
        "request-promise": "^4.2.4",
        "sanitize-html": "^2.3.3",
        "socket.io": "^2.4.1",
        "socket.io-redis": "^5.4.0",
        "socketio-jwt": "^4.5.0",
        "swagger-jsdoc": "^4.3.1",
        "typeorm": "^0.2.28",
        "typeorm-fixtures-cli": "^1.8.0",
        "valid-filename": "^3.1.0",
        "winston": "^3.2.1",
        "xml2js": "^0.4.23",
        "yaml": "^1.3.2",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "@types/aws-sdk": "^2.7.0",
        "@types/bcrypt": "^3.0.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/chai": "^4.2.10",
        "@types/chai-things": "0.0.33",
        "@types/csurf": "1.9.35",
        "@types/expect": "^24.3.0",
        "@types/express": "4.16.1",
        "@types/express-serve-static-core": "4.16.1",
        "@types/express-validator": "^3.0.0",
        "@types/mocha": "^7.0.2",
        "@types/node": "^10.12.26",
        "@types/nodemailer": "^4.6.8",
        "@types/passport": "1.0.0",
        "@types/passport-jwt": "3.0.1",
        "@types/passport-strategy": "^0.2.35",
        "@types/redis": "^2.8.14",
        "@types/request-promise": "^4.1.42",
        "@types/sinon": "^9.0.10",
        "@types/socket.io": "^2.1.2",
        "@types/xml2js": "^0.4.5",
        "@typescript-eslint/eslint-plugin": "^3.7.0",
        "@typescript-eslint/parser": "^3.7.0",
        "chai": "^4.2.0",
        "chai-like": "^1.1.1",
        "chai-sorted": "^0.2.0",
        "chai-things": "^0.2.0",
        "copyfiles": "^2.4.0",
        "dotenv": "^8.2.0",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^6.14.0",
        "eslint-plugin-prettier": "^3.1.4",
        "husky": "^1.3.1",
        "knex": "^0.21.19",
        "lint-staged": "^10.5.4",
        "madge": "^4.0.2",
        "mocha": "^7.2.0",
        "nodemon": "^2.0.6",
        "nyc": "^15.1.0",
        "prettier": "^1.17.0",
        "shx": "^0.3.2",
        "sinon": "^9.2.4",
        "supertest": "^4.0.2",
        "swagger-ui-express": "^4.0.2",
        "ts-mocha": "^7.0.0",
        "ts-mock-imports": "^1.3.1",
        "ts-node": "^8.6.2",
        "typeorm-fixtures-cli": "^1.8.0",
        "typescript": "^3.9.7"
    }
}
