{
  "name": "joolia-client",
  "version": "2.32.2",
  "description": "Joolia Client",
  "author": "Joolia GmbH",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=development --open",
    "start-mock": "start cmd.exe @cmd /k \"cd mockServer | npm run start-server\" | ng serve --proxy-config proxy.conf.json --open",
    "start-test": "ng serve --configuration=test --verbose=true",
    "start-prod": "ng serve --prod",
    "build": "ng build --configuration=development",
    "build-test": "ng build --configuration=test",
    "build-prod": "ng build --prod --base-href /6FEB3BD5-566E-4931-ACE2-5AEA4399B110/ --deploy-url /6FEB3BD5-566E-4931-ACE2-5AEA4399B110/",
    "test": "ng test --configuration=test",
    "test-dev": "ng test --watch --progress",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "pretty": "npm run prettier-run && npm run html-beauty",
    "prettier-run": "prettier --staged --write --config .prettierrc src/**/*.ts src/**/*.js runtime/**/*.js",
    "html-beauty": "js-beautify -config .htmlbeauty -r src/app/**/*.html src/**/*.html runtime/**/*.html",
    "e2e": "ng e2e",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -n \"Joolia Client\" --includes src/documentation",
    "licenses": "license-checker --json > licenses.json && node license-check.js",
    "audit": "npm audit --registry=\"https://registry.npmjs.org/\"",
    "pre-build": "node ./runtime/jenkins/prebuild.js",
    "start-server": "cd mockServer && npm run serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.12",
    "@angular/cdk": "~9.2.4",
    "@angular/common": "~9.1.12",
